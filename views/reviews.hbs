<h1><i class="fas fa-book-reader"> ViewFlix And Spill</i></h1>

<p class="lead">Hello {{name}}!!! See what your friends think!</p>




{{#if reviews}}
    <table class="striped">
        <tbody>
            {{#each reviews}}
                <tr>
                    <tr><strong>{{title}}</strong></tr> <br>
                    <tr>{{body}}</tr>
                </tr>
            {{/each}}
        </tbody>
    </table>
{{/if}}

<a href="/users/logout" class="btn btn-secondary">Logout</a>